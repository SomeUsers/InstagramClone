<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team06.InstagramClone">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.team06.InstagramClone.Home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.team06.InstagramClone.Likes.LikesActivity"></activity>
        <activity android:name="com.team06.InstagramClone.Profile.ProfileActivity"></activity>
        <activity android:name="com.team06.InstagramClone.Search.SearchActivity"></activity>
        <activity android:name="com.team06.InstagramClone.Share.ShareActivity"></activity>
        <activity android:name="com.team06.InstagramClone.Profile.AccountSettingsActivity"></activity>
        <activity android:name="com.team06.InstagramClone.Login.LoginActivity"></activity>
        <activity android:name="com.team06.InstagramClone.Login.RegisterActivity"></activity>


    </application>

</manifest>